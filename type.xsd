    </xsd:element>
    <xsd:element name="delete" type="ftr:requestStockType">
      <xsd:annotation>
        <xsd:documentation>Odstranění záznamu.</xsd:documentation>
      </xsd:annotation>
    </xsd:element>
  </xsd:choice>
</xsd:complexType>


<xsd:simpleType name="stockIdsType">

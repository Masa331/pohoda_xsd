  <xsd:all>
    <xsd:element name="id" type="typ:idType" minOccurs="0"/>
    <xsd:element name="ids" type="typ:idsType" minOccurs="0"/>
    <xsd:element name="accountNo" type="typ:string34" minOccurs="0"/>
    <xsd:element name="bankCode" type="typ:bankCodeType" minOccurs="0"/>
